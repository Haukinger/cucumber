{
  "name": "cucumber-html-formatter",
  "version": "1.0.0",
  "description": "HTML formatter for Cucumber",
  "bin": {
    "cucumber-html-formatter": "bin/cucumber-html-formatter.js"
  },
  "repository": "https://github.com/cucumber/cucumber",
  "author": "Aslak Helles√∏y",
  "license": "MIT",
  "scripts": {
    "nyc": "nyc --reporter=html --reporter=text mocha",
    "test": "mocha",
    "build": "tsc && webpack-cli src/main.tsx",
    "pretty-quick": "pretty-quick --staged"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "cucumber-messages": "4.0.0",
    "jsdom": "^15.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  }
}
